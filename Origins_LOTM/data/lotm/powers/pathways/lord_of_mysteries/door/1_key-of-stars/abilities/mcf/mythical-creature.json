    },
    "added-armor": {
        "type": "origins:attribute",
        "modifiers": [
            {
                "attribute": "minecraft:generic.armor",
                "operation": "addition",
                "value": 30
            },
            {
                "attribute": "minecraft:generic.armor_toughness",
                "operation": "addition",
                "value": 30
            }
        ],
        "update_health": true
    },
    "reduced-hunger": {
        "type": "origins:modify_exhaustion",
        "modifier": {
            "operation": "multiply_total",
            "value": -1
        }
    },
    "added-strength": {
        "type": "origins:attribute",
        "modifiers": [
            {
                "attribute": "minecraft:generic.movement_speed",
